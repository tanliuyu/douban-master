/* pages/movie_login/movie_login.wxss */
.butbox {
  width: 750rpx;
  height: 500rpx;
  background-color: #00B26A;
  display: flex;
  /*设置显示样式**/
  align-items: center;
  /**子view垂直居中*/
  vertical-align: center;
  /**垂直居中*/
  justify-content: center;
  /**内容居中*/
  flex-direction: row;
  position: relative;
}

.but {
  width: 400rpx;
  height: 100rpx;
  border-radius: 100rpx;
  text-align: center;
  background-color: #fff;
  line-height: 100rpx;
  color: #005500;
  font-size: 40rpx;
}
.left{
  display: inline-block;
  font-weight: 900rpx;
  font-size: 35rpx;
  padding: 10rpx;
}
.right{
  display: inline-block;
  position: absolute;
  right: 60rpx;
  padding: 10rpx;
  color: #90A4AE;
}
.toRight{
  padding: 10rpx;
  position: absolute;
  right: 20rpx;
  color: #90A4AE;
  display: inline-block;
}
#to2{
  top: 65rpx;
}
.img{
  width: 60rpx;
  margin: 20rpx;
  margin-top: 65rpx;
  height: 60rpx;
}
.imgbox{
  position: absolute;
  top: 10rpx;
  width: 100rpx;
  height: 200rpx;
  display: inline-block;
}
.ana{
  display: inline-block;
  position: absolute;
  left: 120rpx;
}
.mark{
  color: #979797;
  display: inline-block;
  width: 200rpx;
  position: absolute;
  right: 240rpx;
  top: 65rpx;
  font-size: 30rpx;
}
.open{
  display: inline-block;
  position: absolute;
  right: 60rpx;
  padding: 10rpx;
  background-color: #ebebeb;
  color: #000000;
  text-align: center;
  border-radius: 7rpx;
  font-size: 30rpx;
  top: 65rpx;
}
.anatit{
  vertical-align: top;
}
.k{
  margin-top:40rpx ;
  font-size: 40rpx;
  color: #919191;
}
.box3{
  top: 60rpx;
  position: relative;
}
#tip{
  width: 200rpx;
  height: 200rpx;
  background-color: azure;
  border-radius: 200rpx;
}
#names{
  width: 200rpx;
  text-align: center;
}
#but2{
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  width: 110rpx;
  color: #fff;
  border: 3rpx solid #fff;
  display: flex;
  border-radius: 10rpx;
  padding: 5rpx 10rpx;
  align-items: center;
}
#k{
  color:#6F7CC7;
}
